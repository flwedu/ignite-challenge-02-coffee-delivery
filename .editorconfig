root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = tab
insert_final_newline = true
max_line_length = 100
tab_width = 2
trim_trailing_whitespace = true

[{*.ats,*.cts,*.mts,*.ts}]
indent_style = tab

[{*.htm,*.html,*.sht,*.shtm,*.shtml}]
indent_size = 2
indent_style = tab
